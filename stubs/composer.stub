{
    "refactor": "rector",
    "lint": "pint",
    "test:refactor": "rector --dry-run",
    "test:lint": "pint --test",
    "test:types": "phpstan analyse --ansi",
    "test:unit": "pest --colors=always --coverage --parallel",
    "test": [
        "@test:refactor",
        "@test:lint",
        "@test:types",
        "@test:unit"
    ]
}
